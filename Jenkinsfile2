pipeline {
  agent any	
	environment {
		NAME = 'CHETHAN'
		}
  stages {
	  stage ('BUILD') {
      steps {
        echo "$NAME" 
sh ''' 
sleep 5
echo $NAME
exit 1
  '''
      }  
    }  
    
    stage ('TEST PARALLELE') {
      parallel {
      stage ("TEST ON CHROME") {
      steps {
        echo "This is Test on Chrome Browser" 
	      sh 'sleep 5;exit 0'
      }  
    }  
        stage ("TEST ON SAFARI") {
      steps {
        echo "This is Test on SAFARI Browser" 
	      sh 'sleep 5;exit 0'
      }  
    }
    }
    }
    stage ('DEPLOY') {
	parallel {
	stage ("Server 01") {
      steps {
        echo "This is Deploy to Server 01" 
	      sh 'sleep 5'
      }  
    }
	stage ("Server 02") {
      steps {
        echo "This is Deploy to Server 02" 
	      sh 'sleep 5'
      }  
    }
	stage ("Server 03") {
      steps {
        echo "This is Deploy to Server 03" 
	      sh 'sleep 5'
      }  
    }
  }
}
}
}
