pipeline {
    agent any

    stages {
        stage('Hello') {
            steps {
                echo 'Hello World'
            }
        }

        stage('Build') {
            steps {
                echo 'This is Build'
            }
        }

        stage('Deploy') {
            steps {
                echo 'This is Deploy'
            }
        }

        stage('Test') {
            steps {
                echo 'This is Test'
            }
        }

        stage('Release') {
            steps {
                echo 'This is Release'
            }
        }
    }
}
